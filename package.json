{
	"name": "@aegis-framework/artemis",
	"version": "0.3.23",
	"description": "Aegis Framework Javascript Library",
	"main": "./dist/artemis.min.js",
	"module": "./index.js",
	"repository": {
		"type": "git",
		"url": "https://gitlab.com/AegisFramework/Artemis.git"
	},
	"bugs": {
		"url": "https://gitlab.com/AegisFramework/Artemis/issues"
	},
	"homepage": "https://aegisframework.com/",
	"author": "Diego Islas Ocampo <dev@hyuchia.com>",
	"license": "MIT",
	"keywords": [
		"artemis",
		"dom",
		"javascript",
		"library"
	],
	"scripts": {
		"eslint": "eslint index.js src",
		"jsdoc": "jsdoc ./src -c ./.jsdoc.json && jsdoc2md -f ./src/* -c ./.jsdoc.json >> ./docs/README.md",
		"build": "parcel build ./index.js --global Artemis --out-file artemis.min.js --no-cache --public-url .",
		"watch": "parcel watch ./index.js --global Artemis --out-file artemis.min.js --no-cache --public-url ."
	},
	"devDependencies": {
		"@babel/core": "^7.8.4",
		"eslint": "^6.8.0",
		"jsdoc": "^3.6.3",
		"jsdoc-to-markdown": "^5.0.3",
		"parcel": "^1.12.4"
	},
	"files": [
		"index.js",
		"src/*",
		"dist/*"
	],
	"dependencies": {
		"@babel/plugin-proposal-object-rest-spread": "^7.8.3"
	},
	"browserslist": [
		"> 5%"
	]
}
