{
	"name": "@aegis-framework/artemis",
	"version": "0.2.2",
	"description": "Aegis Framework Javascript Library",
	"main": "./dist/artemis.min.js",
	"module": "./index.js",
	"repository": "https://github.com/AegisFramework/Artemis.git",
	"homepage": "https://aegisframework.com/",
	"author": "Diego Islas Ocampo <dev@hyuchia.com>",
	"license": "MIT",
	"keywords": [
		"artemis",
		"dom",
		"javascript",
		"library"
	],
	"scripts": {
		"eslint": "eslint index.js src",
		"jsdoc": "jsdoc ./src -c ./.jsdoc.json && jsdoc2md -f ./src/* -c ./.jsdoc.json >> ./docs/README.md",
		"build": "parcel build ./index.js --global Artemis --out-file artemis.min.js"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"eslint": "^4.19.1",
		"jsdoc": "^3.5.5",
		"jsdoc-to-markdown": "^4.0.1",
		"parcel": "^1.9.0"
	},
	"files": [
		"index.js",
		"src/*",
		"dist/*"
	]
}